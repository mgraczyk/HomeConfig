#!/bin/sh
export ARCHSTRING="--model_smt 1 --model_b2b 1 --fe_fetch_packets_max 3 --smt_slots 8 --l2cache_fast 0"
if [[ "$1" == *cache* ]]; then
	export ARCHSTRING=:"$ARCHSTRING --cachedebugfile cache.log"
fi

~/dev/arch/bin/sim --timing --quiet --uarchtrace utrace.log --uarchtrace_print_tag_info 1 --pctrace pctrace.log --l2cachetrace l2trace.log *.exe
#~/dev/arch/bin/sim --quiet *.exe

